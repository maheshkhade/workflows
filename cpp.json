{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "enabledManagers": [
    "dockerfile"
  ],
  "dockerfile": {
    "enabled": true,
    "fileMatch": [
      "^Dockerfile$"
    ],
    "groupName": "docker",
    "pinDigests": false
  },
  "vulnerabilityAlerts": {
    "enabled": true
  }
}
